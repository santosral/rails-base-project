<div class="posts">
  <% @foods.each do |food| %>
    <section class="post">
    <%= cl_image_tag(food.id, :width=>100, :height=>100)%>
        <div class="description">
            <%= food.caption %>
        </div>
    </section>
  <% end %>
</div>

<% if @foods_recognized != nil %>
  <% @foods_recognized['recognition_results'].each do |food| %>
    <%= food['name']  %>
  <% end %>
  <%= cl_image_tag(@food.id, :width=>100, :height=>100)%>
<% end %>
