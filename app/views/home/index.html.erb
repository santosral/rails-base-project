<% if user_signed_in? %>
  <h1 class="px-2 text-2xl font-bold text-red-600">Welcome, <%= @cur_user.username %>!</h1><br/>
  <div class="grid px-2 grid-cols-3 gap-3"><%= render 'newsfeed' %></div>
<% elsif nutritionist_signed_in? %>
  <h1 class="px-2 text-2xl font-bold text-red-600">Welcome, <%= @cur_user.username %>!</h1><br/>
  <div class="grid px-2 grid-cols-3 gap-3"><%= render 'newsfeed' %></div>
<% elsif admin_signed_in? %>
  <h1 class="px-2 text-2xl font-bold text-red-600">Hi administrator these doctors are pending for approval</h1>
  <% @nut.each do |nutritionist| %>
    <ul>
      <li><%= nutritionist.username %></li>
      <li><%= nutritionist.email %></li>
      <li><%= nutritionist.license_num %></li>
      <li><%= nutritionist.years_exp %></li>
      <li><%= link_to "Approve", approve_path(nutritionist), method: :get %>
    </ul>
  <% end %>
  <%= link_to 'Log out', destroy_admin_session_path %>
<% else %>
  <div class="landing_page">
    <h1 class='text_hero'>bouffage</h1>
    
  </div>
<% end %>

